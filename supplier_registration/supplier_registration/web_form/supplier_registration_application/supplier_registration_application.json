 "client_script": "// mandatory based on select field 1\nfrappe.web_form.on('have_previous_experience', (field, value) => {\n    if (value == 'Yes') {\n        frappe.web_form.set_field_property('list_of_previous_experience', 'reqd', 1);\n        frappe.web_form.set_field_property('certificates_of_achievements', 'reqd', 1);\n    }\n    else if (value == 'No') {\n        frappe.web_form.set_field_property('list_of_previous_experience', 'reqd', 0);\n        frappe.web_form.set_field_property('certificates_of_achievements', 'reqd', 0);\n    }\n});\n// mandatory based on select field 2\nfrappe.web_form.on('are_you_an_authorized_representative', (field, value) => {\n    if (value == 'Yes') {\n        frappe.web_form.set_field_property('represented_brands_or_companies', 'reqd', 1);\n        frappe.web_form.set_field_property('copy_of_valid_representative_agreements_or_letters', 'reqd', 1);\n    }\n    else if (value == 'No') {\n        frappe.web_form.set_field_property('represented_brands_or_companies', 'reqd', 0);\n        frappe.web_form.set_field_property('copy_of_valid_representative_agreements_or_letters', 'reqd', 0);\n    }\n});\n// date validation 1\nfrappe.web_form.on('expiration_date_business_license', (field, value) => {\n    if (value < frappe.datetime.get_today()) {\n        frappe.msgprint('(\u0635\u0644\u0627\u062d\u064a\u0629 \u062a\u0631\u062e\u064a\u0635 \u0645\u0632\u0627\u0648\u0644\u0629 \u0627\u0644\u0646\u0634\u0627\u0637 \u0644\u0627 \u064a\u0645\u0643\u0646 \u0627\u0646 \u062a\u0643\u0648\u0646 \u062a\u0627\u0631\u064a\u062e \u0631\u062c\u0639\u064a) Expiration Date of valid Business License shall not in past');\n        field.set_value();\n    }\n});\n// date validation 2\nfrappe.web_form.on('expiration_date_commercial_registry', (field, value) => {\n    if (value < frappe.datetime.get_today()) {\n        frappe.msgprint('(\u0635\u0644\u0627\u062d\u064a\u0629 \u0627\u0644\u0633\u062c\u0644 \u0627\u0644\u062a\u062c\u0627\u0631\u064a \u0644\u0627 \u064a\u0645\u0643\u0646 \u0627\u0646 \u062a\u0643\u0648\u0646 \u062a\u0627\u0631\u064a\u062e \u0631\u062c\u0639\u064a)) Expiration Date of valid Commercial Registry shall not in past');\n        field.set_value();\n    }\n});\n\n// Can not save based on field value\nfrappe.web_form.validate = () => {\n    let data = frappe.web_form.get_values();\n    if(data.status!=='Open'){\n        frappe.msgprint('The application under processing....You can not modify the application data');\n        return false;\n    }\n}\n", 
